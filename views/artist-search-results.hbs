<div class="container">
  <div class="card-container">
    {{#each artists}}
    <div class="card">
      <div class="img-container">
        <img src="{{images.0.url}}" alt="image">
      </div>
      <div class="text-container">
        <p>{{name}}</p>
        <a href="/albums/{{id}}">View Albums</a>
      </div>
    </div>  
    {{/each}}
  </div>
</div>  